  <data name="Count" xml:space="preserve">
    <value>CREATE PROCEDURE {T}Count
    @total int OUTPUT
AS
    SET NOCOUNT ON

    SELECT @total = COUNT(*)
      FROM Orden
     WHERE Activo = 1</value>
  </data>
  <data name="Delete" xml:space="preserve">
    <value>CREATE PROCEDURE {T}Delete
	@ID int
AS
	SET NOCOUNT ON

	UPDATE {T}
	   SET Activo = 0
	 WHERE ID = @ID</value>
  </data>
