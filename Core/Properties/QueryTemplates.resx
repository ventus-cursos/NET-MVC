IF EXISTS (SELECT * FROM {T} WHERE ID = @ID)
    UPDATE {T}
       SET  {S}
     WHERE ID = @ID
ELSE
    BEGIN
        INSERT INTO {T} ({C})
        VALUES ({P})

        SET @ID = SCOPE_IDENTITY()
    END</value>
  </data>
